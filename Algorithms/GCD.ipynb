    "def gcd(a,b):\n",
    "    if b>a:\n",
    "        a,b=b,a\n",
    "    r=a%b\n",
    "    if r==0:\n",
    "        return b\n",
    "    else:\n",
    "        return gcd(b,r)"
   ]
